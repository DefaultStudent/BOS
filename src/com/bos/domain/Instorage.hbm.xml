<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bos.domain.Instorage" table="instorage" schema="aistorage">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
        <property name="number">
            <column name="number" sql-type="bigint(50)" not-null="true"/>
        </property>
        <property name="remark">
            <column name="remark" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="areaByAreaid" class="com.bos.domain.Area">
            <column name="areaid" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>