<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bos.domain.Material" table="material" schema="aistorage">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="manufacturer">
            <column name="manufacturer" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="buyprice">
            <column name="buyprice" sql-type="double(10,2)" precision="10" scale="2" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
        <property name="remark">
            <column name="remark" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>