<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bos.domain.Area" table="area" schema="aistorage">
        <id name="id">
            <column name="id" sql-type="varchar(11)" length="11"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(11)" length="11" not-null="true"/>
        </property>
        <property name="mtype">
            <column name="mtype" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="mcount">
            <column name="mcount" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>