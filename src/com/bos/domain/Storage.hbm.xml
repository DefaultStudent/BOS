<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bos.domain.Storage" table="storage" schema="aistorage">
        <id name="id">
            <column name="id" sql-type="varchar(11)" length="11"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="remark">
            <column name="remark" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>